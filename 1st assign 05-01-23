#write a program to print k values in secodary of the given n*n matrix and replace the other values with 0
n=int(input())
m=[]
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    m.append(l)
k=int(input())
for i in range(0,n):
    for j in range(0,n):
        if i==j:
            m[i][j]=k
        else:
            m[i][j]=0
for i in m:
    print(i)
