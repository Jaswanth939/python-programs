#write a progeram to print left diagonal values of the given matrix
r=int(input('enter no of rows'))
c=int(input('enter no of coloumns'))
matrix=[]
for i in range(0,r):
    l=[]
    for j in range(0,c):
        x=int(input())
        l.append(x)
    matrix.append(l)
for i in range(0,r):
    for j in range(0,c):
        if i==j:
            print(matrix[i][j])
